<codelist name="FlowType"   xml:lang="en" complete="1">
    <metadata>
        <name>Flow Type</name>
        <description>DAC/CRS distinction between ODA (official development assistance) and other types of resource flow.</description>
        <url>http://www.oecd.org/dac/stats/dacandcrscodelists.htm</url>
    </metadata>
    <codelist-items>
        <codelist-item>
            <code>10</code>
            <name>ODA</name>
            <name xml:lang="fr">APD</name>
            <description xml:lang="fr">Aide Publique au Développement</description>
        </codelist-item>
        <codelist-item>
            <code>20</code>
            <name>OOF</name>
            <description>Other Official Flows</description>
            <name xml:lang="fr">AASP</name>
            <description xml:lang="fr">Autres Apports du Secteur Public</description>
        </codelist-item>
        <codelist-item>
            <code>30</code>
            <name>Private NGO and other private sources</name>
            <name xml:lang="fr">Apports privés</name>
            <description xml:lang="fr">ONG et autres sources</description>
        </codelist-item>
        <codelist-item>
            <code>35</code>
            <name>Private Market</name>
            <name xml:lang="fr">Apports privés aux conditions du marché</name>
        </codelist-item>
        <codelist-item>
            <code>40</code>
            <name>Non flow</name>
            <description>e.g. GNI</description>
            <name xml:lang="fr">non apports</name>
            <description xml:lang="fr">ex : RNB</description>
        </codelist-item>
        <codelist-item>
            <code>50</code>
            <name>Other flows</name>
            <description>e.g. non-ODA component of peacebuilding operations</description>
            <name xml:lang="fr">Autres apports</name>
            <description xml:lang="fr">ex : élément non APD des opérations de maintien de la paix</description>
        </codelist-item>
    </codelist-items>
</codelist>
